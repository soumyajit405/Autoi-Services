<?xml version="1.0" encoding="UTF-8"?>

 <!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.SmHome.beans">

   <class name="UsrLogInfo" table="usr_log_info">      
      <id name="id" type="int" column="id">
         <generator class="assigned"/>
      </id>
      <property name="homeId" column="home_id" type="string"/>      
      <property name="usrId" column="usr_id" type="string"/>
      <property name="activity" column="activity" type="string"/>
      <property name="logTime" column="time" type="timestamp"/>
     
        
   </class>
</hibernate-mapping>