<?xml version="1.0" encoding="UTF-8"?>

 <!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.SmHome.beans">

   <class name="SensorsBean" table="l4_sensors">      
      <id name="sensorId" type="int" column="id">
         <generator class="assigned"/>
      </id>
         
      <property name="homeId" column="home_id" type="string"/>
      <property name="sensorName" column="sensor_name" type="string"/>
      <property name="sensorValue" column="sensor_value" type="string"/>
       <property name="extraInfo1" column="extra_info1" type="string"/>
      <property name="extraInfo2" column="extra_info2" type="string"/>
            
        
   </class>
</hibernate-mapping>