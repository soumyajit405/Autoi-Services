<?xml version="1.0" encoding="UTF-8"?>

 <!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.SmHome.beans">

	<class name="HomeAdminBean" table="l2_home_admin">
		<id name="homeId" type="string" column="home_id">
			<generator class="assigned" />
		</id>
		
		<property name="email" column="mail_id" type="string" />
		<property name="adminFullName" column="admin_full_name" type="string" />
		<property name="phone" column="phone" type="string" />
		<property name="password" column="password" type="string" />
		<property name="extraInfo1" column="extra_info1" type="string" />
		<property name="extraInfo2" column="extra_info2" type="string" />


	</class>
</hibernate-mapping>