<?xml version="1.0" encoding="UTF-8"?>

 <!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.SmHome.beans">

   <class name="WHDeviceLogBean" table="wh_device_log_info">      
      <id name="id" type="int" column="id">
         <generator class="assigned"/>
      </id>
      <property name="controllerID" column="controller_id" type="string"/>      
      <property name="userID" column="user_id" type="string"/>
       <property name="deviceID" type="int" column="device_id"/>
      <property name="status" column="status" type="string"/>
        <property name="source" column="source" type="string"/>
         <property name="effectiveDate" column="effective_date" type="timestamp"/>
         <property name="onTime" column="on_time" type="int"/>
        
   </class>
</hibernate-mapping>