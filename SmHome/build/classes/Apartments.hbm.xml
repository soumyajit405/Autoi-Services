<?xml version="1.0" encoding="UTF-8"?>

 <!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.SmHome.beans">

   <class name="ApartmentsBean" table="l1_apartment">      
      <id name="id" type="string" column="id">
         <generator class="assigned"/>
      </id>
      <property name="email" column="mail" type="string"/>      
      <property name="builder" column="builder" type="string"/>
      <property name="name" column="name" type="string"/>
      <property name="address" column="address" type="string"/>
      <property name="lat" column="lat" type="string"/>
      <property name="lng" column="lng" type="string"/>
      <property name="extraInfo1" column="extra_info1" type="string"/>
      <property name="extraInfo2" column="extra_info2" type="string"/>
        
   </class>
</hibernate-mapping>