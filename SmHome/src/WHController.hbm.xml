<?xml version="1.0" encoding="UTF-8"?>

 <!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.SmHome.beans">

   <class name="WHControllerBean" table="wh_controller">      
      <id name="id" type="int" column="id">
         <generator class="assigned"/>
      </id>
      <property name="controllerID" column="controller_id" type="string"/>
      <property name="userID" column="user_id" type="string"/>      
      
      <property name="controllerName" column="controller_name" type="string"/>
      
   </class>
</hibernate-mapping>