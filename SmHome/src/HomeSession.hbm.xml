<?xml version="1.0" encoding="UTF-8"?>

 <!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.SmHome.beans">

   <class name="HomeSessionBean" table="home_sessions">      
      <id name="homeId" type="string" column="home_id">
         <generator class="assigned"/>
      </id>
      <property name="sessionId" column="session_id" type="string"/>      
      <property name="effectiveDate" column="effective_date" type="timestamp"/>
        
   </class>
</hibernate-mapping>